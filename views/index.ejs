<!DOCTYPE html>
<html>
  <head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script type ="text/javascript" src="/socket.io/socket.io.js"></script>
    <script type ="text/javascript">
        $(document).ready(function (){ 
          var javascriptCount = 0
          var swiftCount = 0
            var socket = io.connect();
            socket.on('connect', function (data) {
              console.log('BROWSER::WE ARE USING SOCKETS!');
            })
            socket.on('update_javascript', function(data){
              console.log('update javascript');
              javascriptCount++
              $('#javascriptCount').html(javascriptCount)
            })
            socket.on('update_swift',function(data){
              console.log('update swift')
              swiftCount++
              $('#swiftCount').html(swiftCount)
            })
        })
    </script>
  </head>
  <body>
    <h1>JavaScript</h1>
    <p id='javascriptCount'>0</p>
    <h1>Swift</h1>
    <p id = 'swiftCount'>0</p>
  </body>
</html>